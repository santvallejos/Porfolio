---
interface Props {
    Estudio: string;
    Institucion: string;
    fecha: string;
}

const { Estudio, Institucion, fecha} = Astro.props;

import Badge from './Badge.astro';
---

    <li class="ml-5 text-[#90cdf0]">{Estudio}</li>
    <span class="italic opacity-60 text-base block">
    {Institucion} <Badge><a href="/648374a6db665d00026bd230.png" target="_blank">Certificado</a></Badge>
    <span class="block">{fecha}</span>
    </span>