---
interface Props {
    Estudio: string;
    Institucion: string;
    fecha: string;
}

const { Estudio, Institucion, fecha} = Astro.props;
---

    <li class="ml-5 text-blue-500/100">{Estudio}</li>
    <span class="italic dark:opacity-60 text-base block">
    {Institucion}
    <span class="block">{fecha}</span>
    </span>
